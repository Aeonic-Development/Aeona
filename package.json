{
  "name": "freya",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "ts-node-esm src/main.ts",
    "start": "node build/main.js",
    "watch": "npm run format && nodemon --exec ts-node-esm src/main.ts",
    "lint": "npx eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier . --check --print-width 120",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "pretty-quick": "npx -p prettier@latest -p pretty-quick pretty-quick"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/koa": "^1.1.2",
    "@discordx/pagination": "^3.0.0",
    "@discordx/utilities": "^5.0.0",
    "@koa/router": "^12.0.0",
    "discord.js": "^14.3.0",
    "discordx": "^11.1.12",
    "dotenv": "^16.0.2",
    "husky": "^8.0.1",
    "koa": "^2.13.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa__router": "^8.0.11",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-config-xo-typescript": "^0.53.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
